offset
SELECT * FROM test_emp LIMIT 5 OFFSET 10;
offsetWithWhere
SELECT * FROM test_emp WHERE gender = 'F' LIMIT 20 OFFSET 5;
offsetWithOrderBy
SELECT * FROM test_emp ORDER BY emp_no LIMIT 10 OFFSET 15;
offsetWithConstProjection
SELECT 1, 'a' FROM test_emp LIMIT 5 OFFSET 3;
offsetWithContradiction
SELECT gender FROM test_emp WHERE 1 = 2 LIMIT 10 OFFSET 2;
offsetWithFunctions
SELECT salary + 1 s, CONCAT('hello ', first_name) h, EXP(EXP(EXP(salary))) e FROM test_emp LIMIT 10 OFFSET 3;
