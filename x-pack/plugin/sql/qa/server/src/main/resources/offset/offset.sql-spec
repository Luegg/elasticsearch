offsetInInnerSelect
SELECT gender FROM (SELECT gender, languages FROM test_emp LIMIT 2 OFFSET 20);
offsetInOuterSelect
SELECT gender FROM (SELECT gender, languages FROM test_emp) LIMIT 2 OFFSET 20;

